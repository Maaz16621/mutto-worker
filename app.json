{
  "expo": {
    "name": "MUTTO",
    "slug": "mutto-worker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "muttoworker",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.flowcubo.muttoworker"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#FF7D2E"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.flowcubo.muttoworker",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      
      
      
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-bg.png",
          "imageWidth": 375,
          "resizeMode": "cover",
          "backgroundColor": "#f37021"
        }
      ]
    ],
    
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "fa33ae5f-b937-4ab5-a202-8f4e9a5064a7"
      }
    },
    "owner": "maaz16621-org"
  }
}
